pkgname="woof-conky-config"
pkgdesc="Woof's Conky configuration files for Woof OS"
pkgver="1.2.0"
pkgrel=1
arch=('any')
license=('MIT')
depends=('conky')
source=('https://github.com/woof-os/conky/archive/refs/tags/v1.2.0.tar.gz')

sha256sums=("SKIP")

package() {
	cd conky-$pkgver
	install -Dm755 conky.conf $pkgdir/etc/skel/.config/conky/conky.conf
}

